{% extends 'base.html.twig' %}

{% block title %}Inmersiones index{% endblock %}

{% block body %}
    <h1>Inmersiones index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Lugar</th>
                <th>Ciudad</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for inmersione in inmersiones %}
            <tr>
                <td>{{ inmersione.id }}</td>
                <td>{{ inmersione.name }}</td>
                <td>{{ inmersione.lugar }}</td>
                <td>{{ inmersione.ciudad }}</td>
                <td>
                    <a href="{{ path('inmersiones_show', {'id': inmersione.id}) }}">show</a>
                    <a href="{{ path('inmersiones_edit', {'id': inmersione.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('inmersiones_new') }}">Create new</a>
{% endblock %}
